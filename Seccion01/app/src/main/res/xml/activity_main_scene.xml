<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_editor_absoluteY="145dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="258dp"
            android:layout_width="wrap_content"
            android:id="@+id/checkBoxNotify" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="77dp"
            android:layout_width="wrap_content"
            android:id="@+id/switchOnOff"
            motion:layout_constraintTop_toBottomOf="@+id/textNombre"
            android:layout_marginTop="44dp" />
        <Constraint
            android:layout_height="41dp"
            android:layout_marginTop="76dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="248dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="88dp"
            android:layout_marginStart="88dp"
            android:id="@+id/textNombre"
            motion:layout_editor_absoluteY="85dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>